{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "League Season",
                              "component": "SeasonID",
                              "name": "SeasonIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getLeagueSeasonsResult}",
                              "textProperty": "SeasonName",
                              "valueProperty": "SeasonID",
                              "placeholder": "Choose LeagueSeason",
                              "valueType": "int",
                              "type": "dropdown",
                              "name": "SeasonID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${virtualleagueresult.SeasonID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "SeasonIDRequiredValidator",
                              "component": "SeasonID",
                              "text": "SeasonID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Match Day",
                              "component": "MatchDayID",
                              "name": "MatchDayIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getMatchDaysResult}",
                              "textProperty": "MatchDayName",
                              "valueProperty": "MatchDayID",
                              "placeholder": "Choose MatchDay",
                              "valueType": "int",
                              "type": "dropdown",
                              "name": "MatchDayID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${virtualleagueresult.MatchDayID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "MatchDayIDRequiredValidator",
                              "component": "MatchDayID",
                              "text": "MatchDayID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Team",
                              "component": "HomeTeamID",
                              "name": "HomeTeamIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTeamsResult}",
                              "textProperty": "TeamName",
                              "valueProperty": "TeamID",
                              "placeholder": "Choose Team",
                              "valueType": "int",
                              "type": "dropdown",
                              "name": "HomeTeamID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${virtualleagueresult.HomeTeamID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "HomeTeamIDRequiredValidator",
                              "component": "HomeTeamID",
                              "text": "HomeTeamID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Home Score",
                              "component": "HomeScore",
                              "name": "HomeScoreLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "HomeScore",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${virtualleagueresult.HomeScore}"
                            },
                            {
                              "type": "required-validator",
                              "name": "HomeScoreRequiredValidator",
                              "component": "HomeScore",
                              "text": "HomeScore is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Team",
                              "component": "AwayTeamID",
                              "name": "AwayTeamIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getTeamsResult}",
                              "textProperty": "TeamName",
                              "valueProperty": "TeamID",
                              "placeholder": "Choose Team",
                              "valueType": "int",
                              "type": "dropdown",
                              "name": "AwayTeamID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${virtualleagueresult.AwayTeamID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "AwayTeamIDRequiredValidator",
                              "component": "AwayTeamID",
                              "text": "AwayTeamID is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Away Score",
                              "component": "AwayScore",
                              "name": "AwayScoreLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "AwayScore",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${virtualleagueresult.AwayScore}"
                            },
                            {
                              "type": "required-validator",
                              "name": "AwayScoreRequiredValidator",
                              "component": "AwayScore",
                              "text": "AwayScore is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new VirtualLeagueResult!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ConData.createVirtualLeagueResult",
                        "parameters": [
                          {
                            "name": "VirtualLeagueResult",
                            "value": "${virtualleagueresult}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${virtualleagueresult}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${virtualleagueresult}",
                  "visible": "${virtualleagueresult != null}",
                  "itemType": "VirtualLeague.Models.ConData.VirtualLeagueResult"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ConData.getLeagueSeasons",
        "parameters": [],
        "then": [
          {
            "name": "getLeagueSeasonsResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<VirtualLeague.Models.ConData.LeagueSeason>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "ConData.getMatchDays",
        "parameters": [],
        "then": [
          {
            "name": "getMatchDaysResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<VirtualLeague.Models.ConData.MatchDay>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "ConData.getTeams",
        "parameters": [],
        "then": [
          {
            "name": "getTeamsResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<VirtualLeague.Models.ConData.Team>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "virtualleagueresult",
        "type": "set",
        "value": "new VirtualLeague.Models.ConData.VirtualLeagueResult(){}",
        "valueType": "VirtualLeague.Models.ConData.VirtualLeagueResult"
      }
    ]
  },
  "name": "Add Virtual League Result",
  "layout": "Main"
}